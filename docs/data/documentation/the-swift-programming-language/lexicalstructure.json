lineContent":[{"text":"escape-sequence","type":"text"}],"type":"emphasis"},{"text":" ","type":"text"},{"type":"strong","inlineContent":[{"type":"codeVoice","code":"t"}]},{"text":" | ","type":"text"},{"inlineContent":[{"text":"escape-sequence","type":"text"}],"type":"emphasis"},{"text":" ","type":"text"},{"inlineContent":[{"code":"n","type":"codeVoice"}],"type":"strong"},{"text":" | ","type":"text"},{"type":"emphasis","inlineContent":[{"text":"escape-sequence","type":"text"}]},{"type":"text","text":" "},{"type":"strong","inlineContent":[{"code":"r","type":"codeVoice"}]},{"type":"text","text":" | "},{"type":"emphasis","inlineContent":[{"text":"escape-sequence","type":"text"}]},{"text":" ","type":"text"},{"type":"strong","inlineContent":[{"code":"\"","type":"codeVoice"}]},{"type":"text","text":" | "},{"inlineContent":[{"type":"text","text":"escape-sequence"}],"type":"emphasis"},{"type":"text","text":" "},{"inlineContent":[{"type":"codeVoice","code":"'"}],"type":"strong"}]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"escaped-character"}]},{"text":" → ","type":"text"},{"inlineContent":[{"text":"escape-sequence","type":"text"}],"type":"emphasis"},{"type":"text","text":" "},{"type":"strong","inlineContent":[{"type":"codeVoice","code":"u"}]},{"type":"text","text":" "},{"type":"strong","inlineContent":[{"code":"{","type":"codeVoice"}]},{"text":" ","type":"text"},{"inlineContent":[{"type":"text","text":"unicode-scalar-digits"}],"type":"emphasis"},{"type":"text","text":" "},{"inlineContent":[{"type":"codeVoice","code":"}"}],"type":"strong"}]},{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"unicode-scalar-digits","type":"text"}],"type":"emphasis"},{"text":" → Between one and eight hexadecimal digits","type":"text"}]},{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"escaped-newline","type":"text"}],"type":"emphasis"},{"type":"text","text":" → "},{"type":"emphasis","inlineContent":[{"type":"text","text":"escape-sequence"}]},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"inline-spaces"}]},{"inlineContent":[{"type":"text","text":"?"}],"type":"emphasis"},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"line-break"}]}]}]},{"text":"Regular Expression Literals","type":"heading","level":3,"anchor":"Regular-Expression-Literals"},{"inlineContent":[{"text":"A regular expression literal is a sequence of characters","type":"text"},{"text":" ","type":"text"},{"text":"surrounded by slashes (","type":"text"},{"type":"codeVoice","code":"\/"},{"type":"text","text":") with the following form:"}],"type":"paragraph"},{"type":"codeListing","code":["\/<#regular expression#>\/"],"syntax":"swift"},{"inlineContent":[{"type":"text","text":"Regular expression literals"},{"text":" ","type":"text"},{"text":"must not begin with an unescaped tab or space,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"and they can’t contain"},{"type":"text","text":" "},{"type":"text","text":"an unescaped slash ("},{"code":"\/","type":"codeVoice"},{"type":"text","text":"),"},{"text":" ","type":"text"},{"type":"text","text":"a carriage return, or a line feed."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"Within a regular expression literal,","type":"text"},{"type":"text","text":" "},{"text":"a backslash is understood as a part of that regular expression,","type":"text"},{"type":"text","text":" "},{"text":"not just as an escape character like in string literals.","type":"text"},{"text":" ","type":"text"},{"text":"It indicates that the following special character","type":"text"},{"type":"text","text":" "},{"type":"text","text":"should be interpreted literally,"},{"text":" ","type":"text"},{"text":"or that the following nonspecial character","type":"text"},{"type":"text","text":" "},{"text":"should be interpreted in a special way.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"For example,"},{"text":" ","type":"text"},{"type":"codeVoice","code":"\/\\(\/"},{"text":" matches a single left parenthesis","type":"text"},{"type":"text","text":" "},{"type":"text","text":"and "},{"type":"codeVoice","code":"\/\\d\/"},{"text":" matches a single digit.","type":"text"}]},{"inlineContent":[{"type":"text","text":"A regular expression literal delimited by extended delimiters"},{"text":" ","type":"text"},{"text":"is a sequence of characters surrounded by slashes (","type":"text"},{"type":"codeVoice","code":"\/"},{"type":"text","text":")"},{"text":" ","type":"text"},{"type":"text","text":"and a balanced set of one or more number signs ("},{"type":"codeVoice","code":"#"},{"type":"text","text":")."},{"type":"text","text":" "},{"type":"text","text":"A regular expression literal"},{"text":" ","type":"text"},{"text":"delimited by extended delimiters has the following forms:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["#\/<#regular expression#>\/#","","#\/","<#regular expression#>","\/#"]},{"inlineContent":[{"type":"text","text":"A regular expression literal that uses extended delimiters"},{"type":"text","text":" "},{"text":"can begin with an unescaped space or tab,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"contain unescaped slashes ("},{"type":"codeVoice","code":"\/"},{"text":"),","type":"text"},{"type":"text","text":" "},{"text":"and span across multiple lines.","type":"text"},{"type":"text","text":" "},{"text":"For a multiline regular expression literal,","type":"text"},{"text":" ","type":"text"},{"text":"the opening delimiter must be at the end of a line,","type":"text"},{"type":"text","text":" "},{"text":"and the closing delimiter must be on its own line.","type":"text"},{"text":" ","type":"text"},{"text":"Inside a multiline regular expression literal,","type":"text"},{"text":" ","type":"text"},{"text":"the extended regular expression syntax is enabled by default —","type":"text"},{"text":" ","type":"text"},{"text":"specifically, whitespace is ignored and comments are allowed.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If you use more than one number sign to form"},{"text":" ","type":"text"},{"type":"text","text":"a regular expression literal delimited by extended delimiters,"},{"text":" ","type":"text"},{"text":"don’t place whitespace in between the number signs:","type":"text"}],"type":"paragraph"},{"code":["let regex1 = ##\/abc\/##       \/\/ OK","let regex2 = # #\/abc\/# #     \/\/ Error"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"If you need to make an empty regular expression literal,"},{"type":"text","text":" "},{"text":"you must use the extended delimiter syntax.","type":"text"}],"type":"paragraph"},{"style":"note","type":"aside","name":"Grammar of a regular expression literal","content":[{"type":"paragraph","inlineContent":[{"text":"","type":"text"}]},{"inlineContent":[{"inlineContent":[{"type":"text","text":"regular-expression-literal"}],"type":"emphasis"},{"text":" → ","type":"text"},{"type":"emphasis","inlineContent":[{"type":"text","text":"regular-expression-literal-opening-delimiter"}]},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"regular-expression"}]},{"type":"text","text":" "},{"inlineContent":[{"type":"text","text":"regular-expression-literal-closing-delimiter"}],"type":"emphasis"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"text":"regular-expression","type":"text"}]},{"type":"text","text":" → Any regular expression"}]},{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"regular-expression-literal-opening-delimiter","type":"text"}],"type":"emphasis"},{"type":"text","text":" → "},{"type":"emphasis","inlineContent":[{"text":"extended-regular-expression-literal-delimiter","type":"text"}]},{"inlineContent":[{"text":"?","type":"text"}],"type":"emphasis"},{"text":" ","type":"text"},{"type":"strong","inlineContent":[{"type":"codeVoice","code":"\/"}]}]},{"inlineContent":[{"inlineContent":[{"type":"text","text":"regular-expression-literal-closing-delimiter"}],"type":"emphasis"},{"text":" → ","type":"text"},{"inlineContent":[{"type":"codeVoice","code":"\/"}],"type":"strong"},{"text":" ","type":"text"},{"type":"emphasis","inlineContent":[{"type":"text","text":"extended-regular-expression-literal-delimiter"}]},{"type":"emphasis","inlineContent":[{"type":"text","text":"?"}]}],"type":"paragraph"},{"inlineContent":[{"inlineContent":[{"type":"text","text":"extended-regular-expression-literal-delimiter"}],"type":"emphasis"},{"type":"text","text":" → "},{"inlineContent":[{"code":"#","type":"codeVoice"}],"type":"strong"},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"extended-regular-expression-literal-delimiter"}]},{"type":"emphasis","inlineContent":[{"text":"?","type":"text"}]}],"type":"paragraph"}]},{"text":"Operadores","type":"heading","level":2,"anchor":"Operadores"},{"inlineContent":[{"type":"text","text":"The Swift standard library defines a number of operators for your use,"},{"text":" ","type":"text"},{"text":"many of which are discussed in ","type":"text"},{"identifier":"doc:\/\/org.swift.tspl\/documentation\/The-Swift-Programming-Language\/BasicOperators","type":"reference","isActive":true},{"type":"text","text":" "},{"text":"and ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.tspl\/documentation\/The-Swift-Programming-Language\/AdvancedOperators","type":"reference"},{"text":".","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"The present section describes which characters can be used to define custom operators."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Custom operators can begin with one of the ASCII characters"},{"type":"text","text":" "},{"code":"\/","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"="},{"type":"text","text":", "},{"type":"codeVoice","code":"-"},{"type":"text","text":", "},{"type":"codeVoice","code":"+"},{"type":"text","text":", "},{"type":"codeVoice","code":"!"},{"text":", ","type":"text"},{"type":"codeVoice","code":"*"},{"type":"text","text":", "},{"type":"codeVoice","code":"%"},{"text":", ","type":"text"},{"type":"codeVoice","code":"<"},{"type":"text","text":", "},{"code":">","type":"codeVoice"},{"type":"text","text":","},{"text":" ","type":"text"},{"type":"codeVoice","code":"&"},{"type":"text","text":", "},{"type":"codeVoice","code":"|"},{"type":"text","text":", "},{"code":"^","type":"codeVoice"},{"type":"text","text":", "},{"code":"?","type":"codeVoice"},{"type":"text","text":", or "},{"code":"~","type":"codeVoice"},{"type":"text","text":", or one of the Unicode characters"},{"type":"text","text":" "},{"type":"text","text":"defined in the grammar below"},{"type":"text","text":" "},{"text":"(which include characters from the","type":"text"},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"text":"Mathematical Operators","type":"text"}]},{"text":", ","type":"text"},{"type":"emphasis","inlineContent":[{"type":"text","text":"Miscellaneous Symbols"}]},{"type":"text","text":", and "},{"inlineContent":[{"text":"Dingbats","type":"text"}],"type":"emphasis"},{"text":" ","type":"text"},{"type":"text","text":"Unicode blocks, among others)."},{"type":"text","text":" "},{"text":"After the first character,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"combining Unicode characters are also allowed."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"You can also define custom operators"},{"text":" ","type":"text"},{"type":"text","text":"that begin with a dot ("},{"type":"codeVoice","code":"."},{"type":"text","text":")."},{"text":" ","type":"text"},{"type":"text","text":"These operators can contain additional dots."},{"text":" ","type":"text"},{"type":"text","text":"For example, "},{"code":".+.","type":"codeVoice"},{"text":" is treated as a single operator.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"If an operator doesn’t begin with a dot,"},{"text":" ","type":"text"},{"text":"it can’t contain a dot elsewhere.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"For example, "},{"code":"+.+","type":"codeVoice"},{"text":" is treated as","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the "},{"type":"codeVoice","code":"+"},{"text":" operator followed by the ","type":"text"},{"type":"codeVoice","code":".+"},{"type":"text","text":" operator."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"Although you can define custom operators that contain a question mark (","type":"text"},{"type":"codeVoice","code":"?"},{"type":"text","text":"),"},{"text":" ","type":"text"},{"text":"they can’t consist of a single question mark character only.","type":"text"},{"type":"text","text":" "},{"text":"Additionally, although operators can contain an exclamation point (","type":"text"},{"type":"codeVoice","code":"!"},{"type":"text","text":"),"},{"text":" ","type":"text"},{"text":"postfix operators can’t begin with either a question mark or an exclamation point.","type":"text"}]},{"type":"aside","style":"note","name":"Note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The tokens "},{"code":"=","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"->"},{"type":"text","text":", "},{"type":"codeVoice","code":"\/\/"},{"type":"text","text":", "},{"code":"\/*","type":"codeVoice"},{"type":"text","text":", "},{"code":"*\/","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"."},{"type":"text","text":","},{"type":"text","text":" "},{"text":"the prefix operators ","type":"text"},{"type":"codeVoice","code":"<"},{"type":"text","text":", "},{"code":"&","type":"codeVoice"},{"type":"text","text":", and "},{"type":"codeVoice","code":"?"},{"text":",","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the infix operator "},{"type":"codeVoice","code":"?"},{"text":",","type":"text"},{"type":"text","text":" "},{"text":"and the postfix operators ","type":"text"},{"type":"codeVoice","code":">"},{"type":"text","text":", "},{"code":"!","type":"codeVoice"},{"text":", and ","type":"text"},{"type":"codeVoice","code":"?"},{"text":" are reserved.","type":"text"},{"text":" ","type":"text"},{"text":"These tokens can’t be overloaded, nor can they be used as custom operators.","type":"text"}]}]},{"inlineContent":[{"text":"The whitespace around an operator is used to determine","type":"text"},{"text":" ","type":"text"},{"text":"whether an operator is used as a prefix operator, a postfix operator,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"or an infix operator. This behavior has the following rules:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"If an operator has whitespace around both sides or around neither side,","type":"text"},{"type":"text","text":" "},{"text":"it’s treated as an infix operator.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"As an example, the "},{"type":"codeVoice","code":"+++"},{"text":" operator in ","type":"text"},{"type":"codeVoice","code":"a+++b"},{"type":"text","text":" and "},{"code":"a +++ b","type":"codeVoice"},{"text":" is treated as an infix operator.","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"If an operator has whitespace on the left side only,","type":"text"},{"text":" ","type":"text"},{"text":"it’s treated as a prefix unary operator.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"As an example, the "},{"type":"codeVoice","code":"+++"},{"type":"text","text":" operator in "},{"code":"a +++b","type":"codeVoice"},{"type":"text","text":" is treated as a prefix unary operator."}]}]},{"content":[{"inlineContent":[{"text":"If an operator has whitespace on the right side only,","type":"text"},{"type":"text","text":" "},{"text":"it’s treated as a postfix unary operator.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"As an example, the "},{"code":"+++","type":"codeVoice"},{"text":" operator in ","type":"text"},{"type":"codeVoice","code":"a+++ b"},{"type":"text","text":" is treated as a postfix unary operator."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"If an operator has no whitespace on the left but is followed immediately by a dot ("},{"type":"codeVoice","code":"."},{"text":"),","type":"text"},{"type":"text","text":" "},{"type":"text","text":"it’s treated as a postfix unary operator."},{"text":" ","type":"text"},{"text":"As an example, the  ","type":"text"},{"type":"codeVoice","code":"+++"},{"type":"text","text":" operator in "},{"code":"a+++.b","type":"codeVoice"},{"text":" is treated as a postfix unary operator","type":"text"},{"type":"text","text":" "},{"type":"text","text":"("},{"code":"a+++ .b","type":"codeVoice"},{"text":" rather than ","type":"text"},{"code":"a +++ .b","type":"codeVoice"},{"type":"text","text":")."}]}]}],"type":"unorderedList"},{"inlineContent":[{"text":"For the purposes of these rules,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the characters "},{"code":"(","type":"codeVoice"},{"type":"text","text":", "},{"code":"[","type":"codeVoice"},{"type":"text","text":", and "},{"type":"codeVoice","code":"{"},{"type":"text","text":" before an operator,"},{"text":" ","type":"text"},{"type":"text","text":"the characters "},{"code":")","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"]"},{"text":", and ","type":"text"},{"code":"}","type":"codeVoice"},{"text":" after an operator,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"and the characters "},{"type":"codeVoice","code":","},{"type":"text","text":", "},{"type":"codeVoice","code":";"},{"text":", and ","type":"text"},{"type":"codeVoice","code":":"},{"text":" ","type":"text"},{"type":"text","text":"are also considered whitespace."}],"type":"paragraph"},{"inlineContent":[{"text":"If the ","type":"text"},{"type":"codeVoice","code":"!"},{"text":" or ","type":"text"},{"code":"?","type":"codeVoice"},{"type":"text","text":" predefined operator has no whitespace on the left,"},{"type":"text","text":" "},{"text":"it’s treated as a postfix operator,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"regardless of whether it has whitespace on the right."},{"type":"text","text":" "},{"text":"To use the ","type":"text"},{"type":"codeVoice","code":"?"},{"text":" as the optional-chaining operator,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"it must not have whitespace on the left."},{"type":"text","text":" "},{"text":"To use it in the ternary conditional (","type":"text"},{"code":"?","type":"codeVoice"},{"type":"text","text":" "},{"code":":","type":"codeVoice"},{"type":"text","text":") operator,"},{"text":" ","type":"text"},{"type":"text","text":"it must have whitespace around both sides."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"If one of the arguments to an infix operator is a regular expression literal,","type":"text"},{"text":" ","type":"text"},{"text":"then the operator must have whitespace around both sides.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"In certain constructs, operators with a leading ","type":"text"},{"code":"<","type":"codeVoice"},{"type":"text","text":" or "},{"code":">","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"may be split into two or more tokens. The remainder is treated the same way"},{"type":"text","text":" "},{"type":"text","text":"and may be split again."},{"text":" ","type":"text"},{"type":"text","text":"As a result, you don’t need to add whitespace"},{"type":"text","text":" "},{"text":"to disambiguate between the closing ","type":"text"},{"code":">","type":"codeVoice"},{"type":"text","text":" characters in constructs like"},{"type":"text","text":" "},{"code":"Dictionary<String, Array<Int>>","type":"codeVoice"},{"type":"text","text":"."},{"text":" ","type":"text"},{"text":"In this example, the closing ","type":"text"},{"code":">","type":"codeVoice"},{"type":"text","text":" characters aren’t treated as a single token"},{"text":" ","type":"text"},{"type":"text","text":"that may then be misinterpreted as a bit shift "},{"type":"codeVoice","code":">>"},{"type":"text","text":" operator."}]},{"inlineContent":[{"type":"text","text":"To learn how to define new, custom operators,"},{"type":"text","text":" "},{"type":"text","text":"see "},{"text":"doc:AdvancedOperators#Custom-Operators","type":"text"},{"text":" and ","type":"text"},{"text":"doc:Declarations#Operator-Declaration","type":"text"},{"type":"text","text":"."},{"text":" ","type":"text"},{"text":"To learn how to overload existing operators,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"see "},{"text":"doc:AdvancedOperators#Operator-Methods","type":"text"},{"type":"text","text":"."}],"type":"paragraph"},{"type":"aside","style":"note","content":[{"inlineContent":[{"text":"","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"operator"}]},{"text":" → ","type":"text"},{"type":"emphasis","inlineContent":[{"type":"text","text":"operator-head"}]},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"operator-characters"}]},{"type":"emphasis","inlineContent":[{"type":"text","text":"?"}]}],"type":"paragraph"},{"inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"operator"}]},{"type":"text","text":" → "},{"inlineContent":[{"text":"dot-operator-head","type":"text"}],"type":"emphasis"},{"text":" ","type":"text"},{"inlineContent":[{"text":"dot-operator-characters","type":"text"}],"type":"emphasis"}],"type":"paragraph"},{"inlineContent":[{"type":"emphasis","inlineConten