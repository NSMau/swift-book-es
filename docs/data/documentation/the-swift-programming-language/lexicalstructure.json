code":"ExpressibleByIntegerLiteral","type":"codeVoice"},{"text":" protocol,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"and therefore it can be used in the type annotation for the integer literal "},{"code":"42","type":"codeVoice"},{"text":" ","type":"text"},{"type":"text","text":"in the declaration "},{"code":"let x: Int8 = 42","type":"codeVoice"},{"text":".","type":"text"}]},{"name":"Grammar of a literal","content":[{"inlineContent":[{"text":"","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"literal"}],"type":"emphasis"},{"text":" → ","type":"text"},{"inlineContent":[{"type":"text","text":"numeric-literal"}],"type":"emphasis"},{"text":" | ","type":"text"},{"inlineContent":[{"type":"text","text":"string-literal"}],"type":"emphasis"},{"type":"text","text":" | "},{"inlineContent":[{"type":"text","text":"regular-expression-literal"}],"type":"emphasis"},{"text":" | ","type":"text"},{"type":"emphasis","inlineContent":[{"type":"text","text":"boolean-literal"}]},{"type":"text","text":" | "},{"inlineContent":[{"text":"nil-literal","type":"text"}],"type":"emphasis"}]},{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"numeric-literal"}],"type":"emphasis"},{"type":"text","text":" → "},{"inlineContent":[{"code":"-","type":"codeVoice"}],"type":"strong"},{"type":"emphasis","inlineContent":[{"text":"?","type":"text"}]},{"text":" ","type":"text"},{"type":"emphasis","inlineContent":[{"text":"integer-literal","type":"text"}]},{"type":"text","text":" | "},{"type":"strong","inlineContent":[{"type":"codeVoice","code":"-"}]},{"type":"emphasis","inlineContent":[{"type":"text","text":"?"}]},{"type":"text","text":" "},{"type":"emphasis","inlineContent":[{"type":"text","text":"floating-point-literal"}]}]},{"inlineContent":[{"inlineContent":[{"type":"text","text":"boolean-literal"}],"type":"emphasis"},{"type":"text","text":" → "},{"type":"strong","inlineContent":[{"type":"codeVoice","code":"true"}]},{"text":" | ","type":"text"},{"inlineContent":[{"code":"false","type":"codeVoice"}],"type":"strong"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"text":"nil-literal","type":"text"}]},{"type":"text","text":" → "},{"type":"strong","inlineContent":[{"type":"codeVoice","code":"nil"}]}]}],"style":"note","type":"aside"},{"level":3,"text":"Integer Literals","type":"heading","anchor":"Integer-Literals"},{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"Integer literals"}],"type":"emphasis"},{"type":"text","text":" represent integer values of unspecified precision."},{"type":"text","text":" "},{"type":"text","text":"By default, integer literals are expressed in decimal;"},{"text":" ","type":"text"},{"type":"text","text":"you can specify an alternate base using a prefix."},{"text":" ","type":"text"},{"text":"Binary literals begin with ","type":"text"},{"type":"codeVoice","code":"0b"},{"type":"text","text":","},{"text":" ","type":"text"},{"text":"octal literals begin with ","type":"text"},{"code":"0o","type":"codeVoice"},{"type":"text","text":","},{"text":" ","type":"text"},{"text":"and hexadecimal literals begin with ","type":"text"},{"code":"0x","type":"codeVoice"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"text":"Decimal literals contain the digits ","type":"text"},{"type":"codeVoice","code":"0"},{"text":" through ","type":"text"},{"type":"codeVoice","code":"9"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Binary literals contain "},{"code":"0","type":"codeVoice"},{"type":"text","text":" and "},{"code":"1","type":"codeVoice"},{"text":",","type":"text"},{"type":"text","text":" "},{"text":"octal literals contain ","type":"text"},{"code":"0","type":"codeVoice"},{"type":"text","text":" through "},{"type":"codeVoice","code":"7"},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"and hexadecimal literals contain "},{"type":"codeVoice","code":"0"},{"type":"text","text":" through "},{"code":"9","type":"codeVoice"},{"text":" ","type":"text"},{"type":"text","text":"as well as "},{"code":"A","type":"codeVoice"},{"type":"text","text":" through "},{"code":"F","type":"codeVoice"},{"type":"text","text":" in upper- or lowercase."}]},{"type":"paragraph","inlineContent":[{"text":"Negative integers literals are expressed by prepending a minus sign (","type":"text"},{"type":"codeVoice","code":"-"},{"text":")","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to an integer literal, as in "},{"code":"-42","type":"codeVoice"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Underscores ("},{"type":"codeVoice","code":"_"},{"type":"text","text":") are allowed between digits for readability,"},{"type":"text","text":" "},{"type":"text","text":"but they’re ignored and therefore don’t affect the value of the literal."},{"text":" ","type":"text"},{"text":"Integer literals can begin with leading zeros (","type":"text"},{"type":"codeVoice","code":"0"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"but they’re likewise ignored and don’t affect the base or value of the literal."}]},{"type":"paragraph","inlineContent":[{"text":"Unless otherwise specified,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the default inferred type of an integer literal is the Swift standard library type "},{"code":"Int","type":"codeVoice"},{"type":"text","text":"."},{"text":" ","type":"text"},{"text":"The Swift standard library also defines types for various sizes of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"signed and unsigned integers,"},{"text":" ","type":"text"},{"type":"text","text":"as described in "},{"type":"text","text":"doc:TheBasics#Integers"},{"text":".","type":"text"}]},{"style":"note","name":"Grammar of an integer literal","content":[{"inlineContent":[{"text":"","type